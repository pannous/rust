#!/usr/bin/env rust
// import "slices"

nums := [1, 2, 3, 4, 5]

put!("ğŸ¯ LIST METHOD SYNONYMS TEST:")

// Element access synonyms
put!("âœ… first/head/start/begin:")
put!("  first():", nums.first())
put!("  head():", nums.head())
put!("  start():", nums.start())
put!("  begin():", nums.begin())

// Last element synonyms
put!("âœ… last/tail/end/final:")
put!("  last():", nums.last())
put!("  tail():", nums.tail())
put!("  end():", nums.end())
put!("  final():", nums.final())

// Search synonyms  
put!("âœ… contains/includes/has/holds (checking for 3):")
put!("  contains(3):", nums.contains(3))
put!("  includes(3):", nums.includes(3))
put!("  has(3):", nums.has(3))
put!("  holds(3):", nums.holds(3))

// Find synonyms
put!("âœ… find/search/locate (finding 3):")
put!("  find(3):", nums.find(3))
put!("  search(3):", nums.search(3))
put!("  locate(3):", nums.locate(3))

// Filter synonyms with lambda  
evens := nums.filter(x => x%2 == 0)
evens2 := nums.where(x => x%2 == 0)
evens3 := nums.chose(x => x%2 == 0)
evens4 := nums.that(x => x%2 == 0)
evens5 := nums.which(x => x%2 == 0)

put!("âœ… filter/where/chose/that/which (even numbers):")
put!("  filter:", evens)
put!("  where:", evens2)
put!("  chose:", evens3)
put!("  that:", evens4)
put!("  which:", evens5)

put!("\nğŸ‰ SUCCESS! Added natural language synonyms to list methods!")
put!("ğŸŒŸ Easy path achieved - multiple synonyms per method!")