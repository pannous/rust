#!/usr/bin/env rust

fn test_hash_index_with_if_statements() {
	let z = @['a', 'b', 'c'];
	if z#1 != 'a' { panic!("First element failed"); }
	if z#2 != 'b' { panic!("Second element failed"); }
	if z#3 != 'c' { panic!("Third element failed"); }
}

fn main() {
    test_hash_index_with_if_statements();
}
