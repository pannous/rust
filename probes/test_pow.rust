#!/usr/bin/env rust
let a = 2 ** 3;
println!("{}", a);
eq!(a, 8);
eq!(3.0**3.0, 27.0);
eq!(3**3.0, 27.0);
eq!(3.0**3, 27.0);

// Negative base edge cases
eq!((-2)**3, -8);
eq!((-2.0)**3.0, -8.0);
eq!((-3)**2, 9);
eq!((-3.0)**2.0, 9.0);

// Zero edge cases
eq!(0**0, 1);
eq!(0.0**0.0, 1.0);
eq!(0**5, 0);
eq!(5**0, 1);
eq!(5.0**0.0, 1.0);

// Negative exponent edge cases
assert!(2.0**(-1.0) ≈ 0.5);
assert!(2.0**(-2.0) ≈ 0.25);
assert!(10.0**(-1.0) ≈ 0.1);

// One edge cases
eq!(1**100, 1);
eq!(1.0**100.0, 1.0);
eq!((-1)**2, 1);
eq!((-1)**3, -1);

// Fractional exponents (roots)
assert!(4.0**0.5 ≈ 2.0);
assert!(27.0**(1.0/3.0) ≈ 3.0);
assert!(16.0**0.25 ≈ 2.0);

// Mixed sign operations
assert!((-2.0)**2.0 ≈ 4.0);
assert!(2.0**(-3.0) ≈ 0.125);
