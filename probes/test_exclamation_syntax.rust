#!/usr/bin/env rust

fn test_exclamation_syntax_not_available() {
    // Testing exclamation syntax for modifying methods can't work in Rust because it's reserved for macros.
    // This test documents that ! syntax cannot be used for mutating methods in script context
    print!("Testing exclamation syntax for modifying methods can't work in Rust because it's reserved for macros.");
}

fn main() {
    test_exclamation_syntax_not_available();
}
