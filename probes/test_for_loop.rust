#!/usr/bin/env rust
xs := [1,2,3]

sum := 0
for i in [1,2,3] {
		put!(i)
		sum += i
}
eq!( sum , 6);


// While with for-in syntax for slices
numbers := [1, 2, 3]
put!("While with slice:")
for num in numbers {
	printf!("  num = {}\n", num)
}

// While with for-in syntax for maps (iterates key-value pairs)
myMap := @{"a": 1, "b": 2, "c": 3}
put!("While with map:")
for (key, value) in myMap {
	printf!("  {} = {:?}\n", key, value)
}