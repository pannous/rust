#!/usr/bin/env rust

fn test_uppercase_synonyms() {
    eq!("hello".to_uppercase(), "HELLO");
    eq!("hello".upper(), "HELLO");
    eq!("hello".to_upper(), "HELLO");
    eq!("hello".toUpper(), "HELLO");
    eq!("hello".uppercase(), "HELLO");
}

fn test_lowercase_synonyms() {
    eq!("HELLO".to_lowercase(), "hello");
    eq!("HELLO".lower(), "hello");
    eq!("HELLO".to_lower(), "hello");
    eq!("HELLO".toLower(), "hello");
    eq!("HELLO".lowercase(), "hello");
}

fn main() {
    test_uppercase_synonyms();
    test_lowercase_synonyms();
}
