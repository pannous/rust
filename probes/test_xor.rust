#!/usr/bin/env rust
let a = 0b1010;
let b = 0b1100;

// Test xor keyword
let result = a xor b;
assert!(result == 0b0110);

// Test ^ operator (should be equivalent)
assert!(a ^ b == a xor b);

// Test with booleans
let t = true;
let f = false;
assert!(t xor f == true);
assert!(t xor t == false);
assert!(f xor f == false);

// Edge case: xor with all zeros and all ones
assert!(0 ^ 0 == 0);
assert!(0xFF ^ 0xFF == 0);
assert!(0xFF ^ 0x00 == 0xFF);

// Edge case: xor is commutative
assert!(a xor b == b xor a);
assert!(t xor f == f xor t);
assert!(0b1010 ^ 0b0101 == 0b0101 ^ 0b1010);

// Edge case: chained xor operations
assert!(true ^ false ^ true == false);
assert!(true ^ true ^ true == true);
assert!(false ^ false ^ false == false);

// Edge case: xor with powers of 2
assert!(0b1000 ^ 0b0100 == 0b1100);
assert!(0b0001 ^ 0b0010 == 0b0011);
assert!(1 ^ 2 == 3);
assert!(4 ^ 4 == 0);

// Edge case: xor self-inverse property (a xor b xor b == a)
let x = 0b10110;
let y = 0b01101;
assert!(x xor y xor y == x);
assert!(t xor f xor f == t);

// Edge case: xor with negative numbers (two's complement)
let neg = -1i32;
assert!(neg xor 0 == neg);
assert!((-5) xor (-5) == 0);

println!("All xor tests passed!");
