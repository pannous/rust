#!/usr/bin/env rust

fn test_transform_synonyms() {
    let nums = [1, 2, 3];

    let result1 = nums.apply(x => x * 2);
    let result2 = nums.transform(x => x * 2);
    let result3 = nums.convert(x => x * 2);

    let expected = @[2, 4, 6];

    eq!(result1, expected);
    eq!(result2, expected);
    eq!(result3, expected);

    put!("âœ… apply() works");
    put!("âœ… transform() works");
    put!("âœ… convert() works");
    put!("ğŸ›¡ï¸ No map keyword conflict!");
    put!("ğŸ‰ Transform synonyms working safely!");
}

fn main() {
    test_transform_synonyms();
}
