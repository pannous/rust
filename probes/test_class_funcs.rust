#!/usr/bin/env rust
class Point {
    x:int
    y:int
}

fn sum(p:Point)  int {
    return p.x + p.y
}

fn display(p:Point)  {
    put!("x: %d, y: %d\n", p.x, p.y)
}

fn main() {
    t := Point{x: 3, y: 4}
    display(t)
    put!("Sum: %d\n", sum(t))
    // todo: enable as methods:
//     t.display()
//     put!("Sum: %d\n", t.sum())
}