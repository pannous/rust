#!/usr/bin/env rust
// Test only basic methods that don't need imports
eq!( "a"+"1" , "a1");
eq!( "a"+1 , "a1" ); // invalid operation: "a" + 1 (mismatched types untyped string and untyped int)
eq!( "hi".first() , "h");
eq!( "hi".last() , "i");
eq!( "hi".size() , 2);
eq!( "hi".length() , 2);

// Edge cases: Empty string
eq!( "".first() , "");
eq!( "".last() , "");
eq!( "".size() , 0);
eq!( "".length() , 0);

// Edge cases: Single character
eq!( "x".first() , "x");
eq!( "x".last() , "x");
eq!( "x".size() , 1);
eq!( "x".length() , 1);

// Edge cases: Unicode characters
eq!( "cafÃ©".size() , 5);  // byte length
eq!( "cafÃ©".length() , 5);  // byte length
eq!( "cafÃ©".first() , "c");
eq!( "cafÃ©".last() , "Ã©");

eq!( "ä¸–ç•Œ".size() , 6);  // 2 Chinese chars * 3 bytes each
eq!( "ä¸–ç•Œ".first() , "ä¸–");
eq!( "ä¸–ç•Œ".last() , "ç•Œ");

// Edge cases: Emoji (multi-byte)
