#!/usr/bin/env rust

#[test]
fn test_vec_array_equality() {
    let xs := [1, 2, 3]
    let ys := @[1, 2, 3]
    eq!( ys , xs )
}

#[test]
fn test_array_vec_seq_equality() {
    let xs := [1, 2, 3]
    let ys := @[1, 2, 3]
    seq!( xs , ys )
}

fn main() {
    test_vec_array_equality();
    test_array_vec_seq_equality();
}
