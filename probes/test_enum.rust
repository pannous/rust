#!/usr/bin/env rust

enum Status { OK, BAD }
use Status::*;

#[test]
fn test_enum_basic_functionality() {
	put!("OK = %v\n", OK)
	put!("OK = %d\n", OK)
	put!("OK = %s\n", OK) // strict needs String() method
	put!("BAD = %v\n", BAD) // if String() method, else int

	status := OK
	put!("Variable status = %v\n", status)
	eq!( status , OK);
	eqs!( status , "OK");
}

fn main() {
    test_enum_basic_functionality();
}
