#!/usr/bin/env rust
//   string concatenation requires an owned `String` on the left

#[test]
fn test_string_plus_string() {
    let result0 = "a" + "0";
    eq!(result0, "a0");
}

#[test]
fn test_string_plus_integer() {
    let result1 = "a" + 1;
    eq!(result1, "a1");
}

#[test]
fn test_string_plus_larger_integer() {
    let result2 = "value: " + 42;
    eq!(result2, "value: 42");
}

#[test]
fn test_integer_to_string_plus_string() {
    let result3 = 1.to_string() + "b";
    eq!(result3, "1b");
}

#[test]
fn test_string_plus_negative_integer() {
    let result4 = "count: " + (-5);
    eq!(result4, "count: -5");
}

#[test]
fn test_integer_to_string_plus_string_with_spaces() {
    let result5 = 123.to_string() + " items";
    eq!(result5, "123 items");
}

#[test]
fn test_string_plus_float() {
    let result6 = "pi is " + 3.14159;
    eq!(result6, "pi is 3.14159");
}

#[test]
fn test_chained_concatenation() {
    let result7 = "prefix" + 1 + 2 + "suffix";
    eq!(result7, "prefix12suffix");
}

