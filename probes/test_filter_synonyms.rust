#!/usr/bin/env rust
// import "slices"

nums := [1, 2, 3, 4, 5, 6]

// Test all the synonyms
// evens1 := nums.filter(x => x%2 == 0) built-in with different semantics.
// evens2 := nums.where(x => x%2 == 0)    built-in keyword!
evens3 := nums.select(x => x%2 == 0)
evens3 := nums.chose(x => x%2 == 0)
evens4 := nums.that(x => x%2 == 0)
evens5 := nums.which(x => x%2 == 0)

expected := @[2, 4, 6]

eq!( evens3 , expected);
eq!( evens4 , expected);
eq!( evens5 , expected);

put!("âš ï¸ filter() built-in has different semantics")
put!("âš ï¸ where() is a built-in keyword!")
put!("âœ… select() works")
put!("âœ… chose() works")
put!("âœ… that() works")
put!("âœ… which() works")
put!("ğŸ‰ All filter synonyms working perfectly (except filter itself;) !")