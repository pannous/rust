#!/usr/bin/env rust
// Test Unicode comparison operators

// Test ≤ (less than or equal to)
assert!(1 ≤ 2);
assert!(2 ≤ 2);
assert!(!(3 ≤ 2));

// Test ≥ (greater than or equal to)
assert!(2 ≥ 1);
assert!(2 ≥ 2);
assert!(!(1 ≥ 2));

// Test ≠ (not equal to)
assert!(1 ≠ 2);
assert!(!(1 ≠ 1));

// Test … (ellipsis for range)
let range_sum: i32 = (0…5).sum();
assert!(range_sum == 15); // 0+1+2+3+4+5 = 15

// Mix with regular operators todo why does this need special logic in the parser?
assert!(1 <= 2 and 2 ≥ 1);
assert!(1 ≠ 2 and 1 != 2);

println!("All Unicode operator tests passed!");
