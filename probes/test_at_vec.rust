#!/usr/bin/env rust

#[test]
fn test_at_vec_creation() {
    let v = @[1, 2, 3];
    println!("v: {:?}", v);
    assert_eq!(v.len(), 3);
}

#[test]
fn test_at_vec_empty() {
    let empty: Vec<i32> = @[];
    let empty_len = empty.len();
    println!("empty_len: {}", empty_len);
    assert_eq!(empty_len, 0);
}

