#!/usr/bin/env rust

fn test_float_to_int_cast() {
    eq!( 1.0 as int , 1 );
}

fn test_int_to_float_cast() {
    eq!( 3 as float , 3.0 );
}

fn test_int_to_bool_cast() {
    eq!( 1 as bool , true );
}

fn main() {
    test_float_to_int_cast();
    test_int_to_float_cast();
    test_int_to_bool_cast();
}
