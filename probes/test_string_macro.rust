#!/usr/bin/env rustx

fn test_s_macro_basic() {
    let x = s!("hello");
    eq!(x, String::from("hello"));
}

fn test_s_macro_inline() {
    eq!(s!("world"), String::from("world"));
}

fn test_s_macro_with_string_slice() {
    let slice: &str = &"test";
    let y = s!(slice);
    eq!(y, String::from("test"));
}

fn main() {
    test_s_macro_basic();
    test_s_macro_inline();
    test_s_macro_with_string_slice();
}
